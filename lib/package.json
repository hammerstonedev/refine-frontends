{
  "name": "@hammerstone/refine-vue",
  "description": "A powerful, drop in query builder â€” fully integrated with Laravel. See our Laravel backend at https://hammerstone.dev/refine/laravel/docs/main",
  "keywords": [
    "hammerstone",
    "refine",
    "query builder",
    "query",
    "report builder"
  ],
  "source": "src/package.js",
  "module": "./dist/refine-vue.esm.js",
  "homepage": "https://hammerstone.dev",
  "repository": "https://github.com/hammerstonedev/refine-vue2",
  "access": "public",
  "files": [
    "/dist/*",
    "/src/**/*"
  ],
  "version": "0.1.20",
  "scripts": {
    "test": "jest ./tests",
    "build": "yarn run build:vue3 && yarn run build:vue2",
    "build:vue2": "vue-demi-switch 2 && ROLLUP_VUE_VERSION=2 rollup --config rollup.config.js --file dist/vue2/refine-vue.esm.js",
    "build:vue3": "vue-demi-switch 3 && ROLLUP_VUE_VERSION=3 rollup --config rollup.config.js --file dist/vue3/refine-vue.esm.js"
  },
  "peerDependencies": {
    "vue": "vue@^3.0.0-0 || ^2.6.0",
    "@vue/composition-api": "^1.4.2"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.8",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.1",
    "@rollup/plugin-replace": "^3.0.0",
    "@vue/compiler-sfc": "^3.0.0-0",
    "@vue/test-utils": "^1.0.3",
    "autoprefixer": "^10.4.0",
    "core-js": "^3.6.5",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^7.17.0",
    "jest": "^27.4.5",
    "postcss": "^8.4.5",
    "rollup": "^2.61.1",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-vue": "^6.0.0",
    "rollup-plugin-vue2": "npm:rollup-plugin-vue@^5.1.9",
    "tailwindcss": "^3.0.7",
    "vue-template-compiler": "^2.6.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "dependencies": {
    "vue-demi": "^0.12.1"
  }
}
